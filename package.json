{
  "name": "omnisend-node-sdk",
  "version": "1.0.4",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "ajmnz <arnau@ajb.cat>",
  "license": "MIT",
  "packageManager": "yarn@3.6.1",
  "description": "ðŸ”¹ Typesafe Omnisend API SDK for Node.js",
  "keywords": [
    "omnisend",
    "omnisend node",
    "omnisend sdk",
    "omnisend api"
  ],
  "devDependencies": {
    "@types/node": "20.4.6",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "copyfiles": "2.4.1",
    "dotenv-cli": "7.2.1",
    "eslint": "9.2.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "pinst": ">=2",
    "prettier": "3.0.0",
    "release-it": "16.1.3",
    "rimraf": "5.0.1",
    "swagger-typescript-api": "13.0.2",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "axios": "1.4.0",
    "form-data": "4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,ejs}": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write",
    "*.src": "prettier --write"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "clean": "rimraf -rf dist",
    "release": "dotenv -- release-it",
    "build": "tsc && yarn copy:packageJson",
    "copy:packageJson": "copyfiles package.json README.md LICENSE dist",
    "lint": "eslint --fix src",
    "format": "prettier --write src"
  }
}
